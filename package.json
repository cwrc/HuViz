{
  "name": "huviz",
  "version": "0.27.0",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "http://github.com/smurp/huviz.git"
  },
  "config": {
    "port": "9998"
  },
  "main": "lib/huviz.js",
  "dependencies": {
    "@fnando/sparkline": "^0.3.10",
    "async": "~0.2.9",
    "coffee-script": "~1.7.1",
    "components-jqueryui": "git://github.com/components/jqueryui.git",
    "ejs": "2.6.1",
    "express": "~2.5.0",
    "jquery": "^1.10.1",
    "jquery-simulate-ext": "1.3.0",
    "jsoutline": "git://github.com/smurp/jsoutline.git",
    "marked": "^0.3.5",
    "nopt": "~3.0.0",
    "stitch": "~0.3.3"
  },
  "commentedOutDependencies": {},
  "devDependencies": {
    "chai": "~1.10.0",
    "coffeelint": "~1.7.1",
    "grunt": "~0.4.5",
    "grunt-cli": "1.2.0",
    "grunt-contrib-coffee": "~0.12.0",
    "grunt-contrib-watch": "~0.6.1",
    "grunt-express-server": "~0.4.19",
    "grunt-mocha-test": "~0.12.4",
    "grunt-release": "0.9.0",
    "grunt-stitch": "0.1.0",
    "mocha": "~2.0.1",
    "npm-watch": "^0.3.0"
  },
  "watch": {
    "test": {
      "patterns": [
        "js",
        "test",
        "lib",
        "vendor"
      ],
      "extensions": [
        "js",
        "coffee"
      ],
      "quiet": true
    }
  },
  "scripts": {
    "dev": "node ./node_modules/.bin/grunt dev",
    "start": "node server.js --git_commit_hash `git log --format=\"%h\" -n 1` --git_branch_name `git rev-parse --abbrev-ref HEAD`",
    "test": "node_modules/mocha/bin/mocha --reporter dot",
    "watchTest": "npm-watch"
  }
}
