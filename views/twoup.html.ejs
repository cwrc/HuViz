    <div class="outerBox">
      <div id="HUVIZ_TOP" class="HUVIZ_TOP innerBox huviz_wrapper"></div>
      <div id="FLOWER_TOP" class="FLOWER_TOP innerBox huviz_wrapper"></div>
    </div>
    <div style="display:none"> <%- include('tabs/intro') %> </div>
    <script src="/huviz/huviz.js?23234"></script>

    <script src="/js/init_huvis.js"></script>
