    a oa:Annotation ;
    oa:motivatedBy dvrsont:contradiction, dvrsont:factCheck ;
    oa:hasBody youtu:uImk2RgCq_U ;   # the response
    oa:hasTarget [
        oa:hasSource youtu:jNQXAC9IVRw ; # the thing being responded to
        oa:hasSelector [
          a oa:FragmentSelector ;
          dcterms:conformsTo <http://www.w3.org/TR/media-frags/> ;
          rdf:value "t=15,33" ] ] .


